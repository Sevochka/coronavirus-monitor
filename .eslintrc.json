{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "react-app", "airbnb"
    ],
    "settings": {
        "import/resolver": {
          "node": {
            "paths": ["src"]
          }
        }
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        
    }
}
