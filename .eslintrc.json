{
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
},
    "extends": [
      "react-app",
      "prettier",
      "airbnb"
    ],
    "rules": {
      "jsx-quotes": [
        1,
        "prefer-double"
      ],
      "linebreak-style": ["error", "windows"]
    },
    "plugins": [
      "prettier"
    ]
  }